    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Test",
            "outputCapture": "std",
            "program": "${workspaceFolder}/test/${input:test}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Snapshot",
            "outputCapture": "std",
            "program": "${workspaceFolder}/test/${input:test}",
            "env": {
                "TAP_SNAPSHOT": "1"
            }
        }
    ],
    "inputs": [{
        "id": "test",
        "type": "pickString",
        "description": "Pick test to run",
        "options": [
            "mle.test.js",
            "bus.test.js",
            "jose.test.js",
            "jsonrpc.test.js",
            "utError.test.js"
        ]
    }]
}